{
    "name": "agroskurth/archivr",
    "description": "ArchivR is a secure file-synchronization and -backup utility that can be used with a large variety of storage providers.",
    "keywords": ["archivr", "cloud", "sync", "synchronization", "backup", "storage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arne Groskurth",
            "email": "arne.groskurth@gears9.de"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/finder": "^3.2",
        "ramsey/uuid": "^3.6",
        "symfony/console": "^3.3",
        "zendframework/zend-stdlib": "^3.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "symfony/filesystem": "^3.3"
    },
    "autoload": {
        "psr-4": {"Archivr\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Archivr\\Test\\": "tests/"}
    },
    "scripts": {
        "build": "@php -d phar.readonly=0 bin/build",
        "test": "phpunit --colors=always --verbose"
    },
    "bin": [
        "bin/archivr"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
